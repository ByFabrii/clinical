{
  "name": "dental-records-backend",
  "version": "1.0.0",
  "description": "Backend API para Sistema de Expedientes ClÃ­nicos Dentales - Cumplimiento NOM-024 y NOM-013",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc && tsc-alias",
    "build:watch": "tsc --watch",
    "build:vercel": "tsc --project tsconfig.production.json && tsc-alias",
    "vercel-build": "npm run build",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest src/__tests__/e2e --run",
    "test:e2e:verbose": "vitest src/__tests__/e2e -- --reporter=verbose",
    "test:e2e:login": "vitest src/__tests__/e2e/login.e2e.test.ts --run",
    "test:e2e:register": "vitest src/__tests__/e2e/register.e2e.test.ts --run",
    "test:e2e:clinics": "vitest src/__tests__/e2e/clinics.e2e.test.ts --run",
    "test:e2e:patients": "vitest src/__tests__/e2e/patients.e2e.test.ts --run",
    "test:integration": "vitest src/__tests__/integration --run",
    "test:integration:clinics": "vitest src/__tests__/integration/clinics --run",
    "test:unit": "vitest src/__tests__/unit --run",
    "test:unit:controllers": "vitest src/__tests__/unit/controllers --run",
    "test:clinics": "npm run test:unit:controllers && npm run test:integration:clinics && npm run test:e2e:clinics",
    "test:patients": "npm run test:unit:controllers && npm run test:integration:patients && npm run test:e2e:patients",
    "postinstall": "npm run build"
  },
  "keywords": [
    "dental",
    "medical-records",
    "healthcare",
    "api",
    "supabase",
    "nom-024",
    "nom-013"
  ],
  "author": "Fabrizzio Dev",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "@types/nodemailer": "^7.0.1",
    "@vercel/node": "^3.2.29",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
